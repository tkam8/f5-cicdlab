{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.10.0",
        "id": "demo-servicediscovery",
        "label": "{{ as3_service_name }}",
        "remark": "HTTPS with pool service-discovery",
        "updateMode": "selective",
        "controls": {
            "class": "Controls",
            "logLevel": "debug"
        },
        "{{ partition }}": {
            "class": "Tenant",
            "{{ as3_service_name }}": {
                "class": "Application",
                "template": "https",
                "serviceMain": {
                    "class": "Service_HTTPS",
                    "virtualAddresses": [
                        "{{ as3_virtual_address }}"
                    ],
                    "snat": "auto",
                    "pool": "{{ as3_pool_name }}",
                    "serverTLS": "webtls"
                },
                "{{ as3_pool_name }}": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [{
                        "servicePort": 80,
                        "addressDiscovery": "aws",
                        "updateInterval": 50,
                        "tagKey": "{{APP_TAG_KEY}}",
                        "tagValue": "{{APP_TAG_VALUE}}",
                        "addressRealm": "private",
                        "region": "ap-northeast-1",
                        "accessKeyId": "{{AWS_ACCESSKEY}}",
                        "secretAccessKey": "{{AWS_SECRETKEY}}",
                        "credentialUpdate": false,
                        "shareNodes": true
                    }]
                },
                "webtls": {
                    "class": "TLS_Server",
                    "certificates": [
                        {
                            "certificate": "webcert"
                        }
                    ]
                },
                "webcert": {
                    "class": "Certificate",
                    "certificate": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----",
                    "privateKey": "-----BEGIN RSA PRIVATE KEY-----\\n-----END RSA PRIVATE KEY-----"
                }
            }
        }
    }
}
